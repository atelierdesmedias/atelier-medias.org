# gitignore file

# Mac
.DS_Store

# Ignore ide files
.idea/
.cache/

# Logs
logs
*.log

# Dependency directory
# https://www.npmjs.org/doc/misc/npm-faq.html#should-i-check-my-node_modules-folder-into-git
node_modules/

# ------------------------------------------------------------------------------ SRC

# atoms.ts
src/atoms/atoms.ts
# env properties selected
properties/.envName.js
.env

# ------------------------------------------------------------------------------ DIST

dist/user/themes/adm/assets

dist/vendor/*
dist/backup/*
!dist/backup/.gitkeep
dist/cache/*
!dist/cache/.gitkeep
dist/logs/*
!dist/logs/.gitkeep
dist/tmp/*
!dist/tmp/.gitkeep
dist/images/*
!dist/images/.gitkeep
dist/user/plugins/**/vendor/*
#dist/user/config/security.yaml

# ------------------------------------------------------------------------------ OLD DIST

# ignore vendor folder create by composer
dist-wp/app/vendor/*
# ignore webpack compile assets folder
dist-wp/public/assets/
# auto generated dot env
dist-wp/.env
# auto generated config php
dist-wp/public/themes/wordplate/config.php
# php config file expose node env variable
dist-wp/config.php

