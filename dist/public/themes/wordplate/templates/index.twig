{% spaceless %}
<!doctype html>
<!--[if lt IE 7]><html class="no-js no-svg ie ie6 lt-ie9 lt-ie8 lt-ie7" {{ site.language_attributes }}> <![endif]-->
<!--[if IE 7]><html class="no-js no-svg ie ie7 lt-ie9 lt-ie8" {{ site.language_attributes }}> <![endif]-->
<!--[if IE 8]><html class="no-js no-svg ie ie8 lt-ie9" {{ site.language_attributes }}> <![endif]-->
<!--[if gt IE 8]><!--><html class="no-js no-svg" {{ site.language_attributes }}> <!--<![endif]-->
<head>
	<meta charset="{{ site.charset }}" />
	<title>{{ wp_title ? wp_title : site.name }}</title>
	<meta name="description" content="{{ site.description }}">
	<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	{{ function('wp_head') }}
	{% block head %}{% endblock %}
</head>

<body>
	<div class="AppView">
		{# Header présent sur toutes les pages #}
        {%
            include "./templates/MainHeader.twig"
            with { 'classElement': 'AppView_mainHeader' }
        %}

        {# content #}
        {% block content %}
			Sorry, no content here.
        {% endblock %}


        {# Footer présent sur toutes les pages #}
        {%
            include "templates/MainFooter.twig"
            with { 'classElement': 'AppView_mainFooter' }
        %}
	</div>

	{% block footer %}
		{{ function('wp_footer') }}
	{% endblock %}
</body>

</html>
{% endspaceless %}
